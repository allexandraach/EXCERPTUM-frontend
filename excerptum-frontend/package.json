{
  "name": "excerptum-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "axios": "^1.7.8",
    "clipboard-copy": "^4.0.1",
    "dayjs": "^1.11.13",
    "eslint": "^8.57.1",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-app": "^1.1.2",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.3.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^7.0.1",
    "react-toastify": "^10.0.6",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.4.16",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "dev": "vite --mode test",
    "build": "npx vite build --mode production --outDir dist/prod/front",
    "build-local": "vite build --mode local --outDir dist/local",
    "build-development": "vite build --mode development --outDir dist/dev/front",
    "build-production": "vite build --mode production --outDir dist/prod/front",
    "preview": "vite preview",
    "serve": "serve dist",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "vite": "^6.0.3"
  }
